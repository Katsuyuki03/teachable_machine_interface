{
    "manifest_version": 3,
    "name": "youtube-stop",
    "description": "youtubeを止めたり再生したりする",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["content.js"]
      }
    ]
  }