{
    "manifest_version": 3,
    "name": "ミテコンチューブ",
    "description": "youtubeを止めたり再生したりする",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": [
          "content.js"]
      }
    ]
  
  }