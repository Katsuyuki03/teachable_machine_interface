{
    "manifest_version": 3,
    "name": "youtube-stop",
    "description": "youtubeを止めたり再生したりする",
    "version": "1.0",
    "permissions": ["scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": ["http://*/*", "https://*/*"],
    "icons": {
      "16": ""
    }
  }